{
    "name": "benjaber-98/laravel-https",
    "description": "This is my package LaravelHttps",
    "keywords": [
        "Benjaber-98",
        "laravel",
        "laravel-https"
    ],
    "homepage": "https://github.com/Benjaber-98/laravel-https",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Ben Jabir",
            "email": "mahmoudbebjabir@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "*"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Benjaber98\\LaravelHttps\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjaber98\\LaravelHttps\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benjaber98\\LaravelHttps\\LaravelHttpsServiceProvider"
            ],
            "aliases": {
                "LaravelHttps": "Benjaber98\\LaravelHttps\\LaravelHttpsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
